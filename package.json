{
    "name": "kitten-cloud-function",
    "version": "1.1.1-alpha.3",
    "description": "用于编程猫源码云功能（云变量、云列表等）的客户端工具",
    "main": "./dist/src/src/wrapper/kitten-cloud-function-npm-package.js",
    "types": "./dist/types/src/wrapper/kitten-cloud-function-npm-package.d.ts",
    "browser": "./dist/src/src/wrapper/kitten-cloud-function-browser.js",
    "bin": {
        "Kitten-Cloud-Function": "./dist/src/src/wrapper/kitten-cloud-function-command-line-interface.js",
        "kcf": "./dist/src/src/wrapper/kitten-cloud-function-command-line-interface.js"
    },
    "directories": {
        "doc": "docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/s-lightning/Kitten-Cloud-Function.git"
    },
    "author": "SLIGHTNING",
    "license": "AGPL-3.0",
    "keywords": [
        "源码云功能",
        "kitten-cloud",
        "kitten-cloud-var",
        "kitten-cloud-variable",
        "kitten-cloud-list",
        "cloud-var",
        "cloud-variable",
        "cloud-list",
        "云变量",
        "云列表",
        "kitten",
        "源码编辑器",
        "codemao",
        "bcm",
        "编程猫",
        "菠菜面",
        "你猫",
        "毛毡"
    ],
    "bugs": {
        "url": "https://github.com/s-lightning/Kitten-Cloud-Function/issues"
    },
    "homepage": "https://s-lightning.github.io/Kitten-Cloud-Function/",
    "scripts": {
        "build:coco-widget": "webpack --config coco-widget.webpack.config.js",
        "build:coco-widget--no-modification-restriction": "webpack --config coco-widget.webpack.config.js --env noModificationRestriction",
        "build:browser": "webpack --config browser.webpack.config.js",
        "build:user-script": "webpack --config user-script.webpack.config.js",
        "build:package": "tsc --module commonjs --moduleResolution node",
        "build:docs": "typedoc --sort static-first --sort source-order --plugin typedoc-material-theme",
        "build": "npm run build:coco-widget && npm run build:coco-widget--no-modification-restriction && npm run build:browser && npm run build:user-script && npm run build:package && npm run build:docs"
    },
    "dependencies": {
        "@ungap/promise-any": "^1.1.1",
        "axios": "^1.7.2",
        "commander": "^12.1.0",
        "crypto-js": "^4.2.0",
        "diff": "^5.2.0",
        "slightning-coco-widget": "1.0.1-alpha.11"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.26.0",
        "@types/diff": "^7.0.0",
        "babel-loader": "^9.2.1",
        "slightning-coco-widget--webpack": "1.0.2-alpha.4",
        "terser-webpack-plugin": "^5.3.10",
        "ts-loader": "^9.5.1",
        "typedoc": "^0.26.6",
        "typedoc-material-theme": "^1.1.0",
        "webpack": "^5.93.0",
        "webpack-cli": "^5.1.4"
    },
    "engines": {
        "node": ">=14"
    }
}
