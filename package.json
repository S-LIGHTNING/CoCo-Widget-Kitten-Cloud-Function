{
    "name": "kitten-cloud-function",
    "version": "1.1.1-alpha.1",
    "description": "用于编程猫源码云功能（云变量、云列表等）的客户端工具",
    "main": "index.js",
    "types": "index.ts",
    "directories": {
        "doc": "docs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/s-lightning/Kitten-Cloud-Function.git"
    },
    "author": "SLIGHTNING",
    "license": "AGPL-3.0",
    "keywords": [
        "源码云功能",
        "kitten-cloud",
        "kitten-cloud-var",
        "kitten-cloud-variable",
        "kitten-cloud-list",
        "cloud-var",
        "cloud-variable",
        "cloud-list",
        "云变量",
        "云列表",
        "kitten",
        "源码编辑器",
        "codemao",
        "bcm",
        "编程猫",
        "菠菜面",
        "你猫",
        "毛毡"
    ],
    "bugs": {
        "url": "https://github.com/s-lightning/Kitten-Cloud-Function/issues"
    },
    "homepage": "https://s-lightning.github.io/Kitten-Cloud-Function/",
    "scripts": {
        "build--coco-widget": "webpack --config coco-widget.webpack.config.js",
        "build--tampermonkey": "webpack --config tampermonkey.webpack.config.js",
        "build--package": "tsc",
        "build": "npm run build--coco-widget && npm run build--tampermonkey && npm run build--package",
        "make-docs": "typedoc --sort static-first --sort source-order --plugin typedoc-material-theme",
        "*": "npm run build && npm run make-docs"
    },
    "dependencies": {
        "@types/diff": "^5.2.1",
        "axios": "^1.7.2",
        "diff": "^5.2.0",
        "slightning-coco-widget": "^1.0.1-alpha.2"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/plugin-transform-runtime": "^7.24.7",
        "@babel/preset-env": "^7.25.3",
        "@babel/preset-typescript": "^7.24.7",
        "babel-loader": "^9.1.3",
        "slightning-coco-widget--webpack": "^1.0.0",
        "typedoc": "^0.26.6",
        "typedoc-material-theme": "^1.1.0",
        "webpack": "^5.93.0",
        "webpack-cli": "^5.1.4"
    }
}
